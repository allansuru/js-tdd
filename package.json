{
  "name": "js-tdd",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/*.js",
    "ap": "echo 'OI'",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test-cat": "npm test -- --reporter=nyan",
    "test:coverage": "nyc npm test",
    "build": "./node_modules/.bin/webpack --colors --progress",
    "watch": "npm run build -- --watch",
    "server": "./node_modules/.bin/webpack-dev-server --inline --open",
    "prepush": "npm run lint && npm run test:coverage"
  },
  "nyc": {
    "check-coverage": true,
    "functions": 80,
    "lines": 80,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allansuru/js-tdd.git"
  },
  "author": "Allan Passos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allansuru/js-tdd/issues"
  },
  "homepage": "https://github.com/allansuru/js-tdd#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "v8.0.0-beta.0",
    "@babel/preset-react": "^7.0.0-beta.40",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "js-config-webpack-plugin": "^2.0.1",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "",
      "pre-push": "npm run prepush"
    }
  },
  "dependencies": {
    "generate-webpack-config": "^2.0.0",
    "global": "^4.4.0",
    "node-fetch": "^2.6.1",
    "ramda": "^0.27.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
